{
  "name": "acquit",
  "version": "0.0.3",
  "description": "Parse BDD-style tests (Mocha, Jasmine) to generate documentation",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/*.test.js",
    "test-coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*.test.js"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/vkarpov15/acquit.git"
  },
  "dependencies": {
    "commander": "2.5.0",
    "esprima": "https://github.com/ariya/esprima/archive/85fc2f4b6ad109a86d80d9821f52b5b38d0105c0.tar.gz",
    "marked": "0.3.2",
    "mocha": "2.0.1",
    "underscore": "1.5.2"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireMultipleVarDecl": null,
    "disallowMultipleVarDecl": true
  },
  "devDependencies": {
    "istanbul": "0.3.5",
    "jscs": "1.9.0"
  }
}
